<div id="list-example" class="list-group">
  <% @bookings.each do |booking| %>
    <a class="list-group-item list-group-item-action" href="#list-item-<%= booking.id %>"><%= booking.garden.name %></a>
  <% end %>
</div>
<div data-spy="scroll" data-target="#list-example" data-offset="0" class="scrollspy-example">
  <% @bookings.each do |booking| %>
    <h4 id="#list-item-<%= booking.id %>"> <%=booking.garden.name  %> </h4>
    <p>Start Date: <%= booking.start_date.strftime("%m/%d/%Y")%></p>
    <p>End Date: <%= booking.end_date.strftime("%m/%d/%Y")  %></p>
  <% end %>
</div>

<div id="list-example" class="list-group">
