<div class="body">
  <%= render 'shared/home_navbar' %>
  <div class="card">
    <div class="tag">
      <h1>Rent An Outside Space...</h1>
    </div>
    <div class="search-bar">
      <div>Where</div>
      <div class="searching">
        <%= form_tag("#", method: :get) do %>
          <%= text_field_tag :search, nil, class: "input-field" %>
          <div class="flex-btns" id="green-button">
            <%= submit_tag 'Search', class: "btn btn-success" %>
            <button type="button" class="btn btn-success">Browse All</button>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
